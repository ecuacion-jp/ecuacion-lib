<!DOCTYPE suppressions PUBLIC
     "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
     "https://checkstyle.org/dtds/suppressions_1_2.dtd">
 
<suppressions>

	<!-- license-mavevn-plugin creates just above the package declaration and violates this check -->
	<suppress checks="EmptyLineSeparator" message="'package'.*"/>

	<!-- To "empty" a line a whitespace need to be removed manually.    -->	
	<suppress checks="RequireEmptyLineBeforeBlockTagGroup" message="Javadoc.*'@(param|return|see)'.*" />
	
	<!-- javadoc not created in "*.internal" package  -->
	<suppress checks=".*Javadoc.*" files=".*.internal..*.java" />
	
	<!-- excluding test -->
	<suppress checks=".*" files="Test.*" />
	<suppress checks=".*" files=".*Test.java" />
	
</suppressions>
